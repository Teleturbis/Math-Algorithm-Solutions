{
  "name": "math-algorithm-solutions",
  "version": "1.0.0",
  "description": "A collection of JavaScript implementations solving various mathematical problems and challenges.",
  "main": "src/index.js",
  "scripts": {
    "prepare": "husky",
    "dev": "npx nodemon",
    "test": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
