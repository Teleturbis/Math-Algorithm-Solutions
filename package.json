  "license": "MIT",
  "eslintIgnore": [
    "tests/check-coverage.js"
  ],
  "lint-staged": {
    "*.ts": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.29",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
